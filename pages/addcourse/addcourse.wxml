        <view class="weui-cells__tips" >{{infoMess}}</view> 
        <view class="page__td_spacing page__bd_spacing">
          <button class="weui-btn" type='primary' plain='true' bindtap="createCourseBtnClick">创建</button>
          <button class="weui-btn" type='warn' plain='true' bindtap="resetBtnClick">重置</button>
        </view>
  <view>{{infoMess}}</view>
